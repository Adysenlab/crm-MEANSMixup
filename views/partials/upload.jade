p
    #main-content
        h3 Upload
        body
            - if (typeof(target_path) != 'undefined')
                p=target_path
            table(width='300', border='0', align='center', cellpadding='0', cellspacing='0', bgcolor='#CCCCCC')
                tr
                    form(name='formUpload', method='post', action='uploadf',enctype="multipart/form-data")
                        td
                            table(width='100%', border='0', cellpadding='3', cellspacing='1', bgcolor='#FFFFFF')
                                tr
                                    td(colspan='3')
                                        strong MAS Upload
                                tr
                                    td Password
                                    td :
                                    td
                                        input#mypassword(name='mypassword', type='text',value='')
                                tr
                                    td Description of file(s)
                                    td :
                                    td
                                        input#updesc(name='updesc', type='text',value='')
                                tr
                                    td File(s) to upload
                                    td :
                                    td
                                        input#thumbnail(name='thumbnail', type='file',multiple='multiple')
                                tr
                                    td
                                    td
                                    td
                                        input(type='submit', name='Submit', value='Upload' ,id="fileToUpload",onchange="fileSelected()" )
                                tr
                                    td

                                        a(href='http://www.markadjustment.com/server/uploads/man_silhouette_clip_art_9510.jpg') downloadTest
                                        a(href='http://www.markadjustment.com/uploads/man_silhouette_clip_art_9510.jpg') downloadTest2
                                        a(href='http://www.markadjustment.com/server/uploads/handshake.gif') downloadTest
                                        a(href='http://www.markadjustment.com/uploads/handshake.gif') downloadTest2
